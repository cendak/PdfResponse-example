{
	"name": "jkuchar/pdfresponse-example",
	"description": "Example usage of PdfResponse project.",
	"homepage": "http://github.com/jkuchar/PdfResponse",
	"license": ["New BSD"],
	"authors": [
		{
			"name": "Jan KuchaÅ™",
			"homepage": "http://jankuchar.cz"
		}
	],
	"config": {
		"vendor-dir": "libs"
	},
	"repositories": [
		{
            "type": "package",
            "package": {
                "name": "simplehtmldom/simplehtmldom",
                "version": "dev-master",
				"require": {
					"php": ">= 5.0.0"
				},
				"autoload": {
					"classmap":["simple_html_dom.php"]
				},
                "source": {
                    "url": "http://svn.code.sf.net/p/simplehtmldom/code",
                    "type": "svn",
					"reference": "trunk"
                }
            }
        }
    ],
	"require": {
		"php": ">= 5.3.0",
		"nette/nette": ">= 2.0.7",
		"jkuchar/PdfResponse": "dev-master",
		"simplehtmldom/simplehtmldom": "dev-master"
	},
    "require-dev": {
        "nette/tester": "@dev"
	}
}
